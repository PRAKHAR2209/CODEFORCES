import java.util.*;
 
public class Solution{
	
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		int n= sc.nextInt();
		int x=0;
	    int[] arr = new int[3];
	   for(int i=0;i<n;i++){
	       int sum=0;
	       for(int j=0;j<3;j++){
	           arr[j]= sc.nextInt();
	           sum+=arr[j];
	       }
	       if(sum>=2)
	            {x++;}
	   }
	       
	   System.out.println(x);
    }
	    
	
 }
